    "@types/mathjs": "^6.0.5",
    "@types/xml2js": "^0.4.6",
    "cypress": "7.7.0",
    "jest-runner": "^27.0.4",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "tslib": "^2.3.1",
